{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"],
      "@scripts/*": ["src/scripts/*"],
      "@utilities/*": ["src/utilities/*"]
    },
    "types": ["unplugin-icons/types/astro"],
    "allowJs": true
  },
  "exclude": ["dist", "public"]
}
